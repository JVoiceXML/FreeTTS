#!/bin/sh
# execute freetts with the correct classpath
TOP=`dirname $0`/../
LIBDIR="$TOP/lib"
JARS="$LIBDIR/freetts.jar:$LIBDIR/en_us.jar"
if [ -z "${JAVA_HOME}" ] ; then
    JAVA_HOME=/lab/speech/java/j2sdk1.4.0
fi
CLASS_PATH="$JARS:$TOP/demo/classes"
echo $CLASS_PATH
${JAVA_HOME}/bin/java -Xms64m -ea -cp "$CLASS_PATH" \
	-Dmbrola.base=${MBROLA_DIR} \
	$@
