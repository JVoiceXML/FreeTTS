#!/bin/sh
# execute freetts with the correct classpath, using default heapsize
BASE=`dirname $0`/../
LIBDIR="$BASE/lib"
JARS="$LIBDIR/freetts.jar"
if [ -z "${JAVA_HOME}" ] ; then
    JAVA_HOME=/lab/speech/java/j2sdk1.4.0
fi
${JAVA_HOME}/bin/java -ea -cp "$JARS:$TOP/demo/classes" \
	-Dmbrola.base=${MBROLA_DIR} \
	$@
